{
  "name": "haven-components",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/haven-components.es.js",
      "require": "./dist/haven-components.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/haven-components.umd.js",
  "module": "./dist/haven-components.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.6",
    "@testing-library/cypress": "^9.0.0",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-react": "^2.1.0",
    "chartjs-plugin-annotation": "^2.1.2",
    "cypress": "^12.5.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-linter": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.6",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/efost/haven-components"
  },
  "keywords": [],
  "author": "Eric Foster",
  "bugs": {
    "url": "https://github.com/efost/haven-components/issues"
  },
  "homepage": "https://github.com/efost/haven-components/#readme",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "chart.js": "^4.2.0",
    "react-chartjs-2": "^5.2.0"
  }
}
